NAME_TABLE:


C_Function_Name:       cm_res_emo
Spice_Model_Name:      res_emo
Description:           "resistor with emotions"


PORT_TABLE:


Port_Name:             connection1        connection2    
Description:           "connection1"      "connection2"  
Direction:             in                 in              
Default_Type:          v                  v            
Allowed_Types:         [v,vd,i,id,vnam]   [v,vd,i,id,vnam] 
Vector:                 no                 no              
Vector_Bounds:          -                  -               
Null_Allowed:          no                 no             



PARAMETER_TABLE:


Parameter_Name:     res_resistance              res_power 
Description:        "resistor resistance"       "resistor power rating"
Data_Type:          real                        real   
Default_Value:      1.0                         1.0   
Limits:             -                           -       
Vector:              no                          no     
Vector_Bounds:       -                           -      
Null_Allowed:       yes                         yes    


                 

PARAMETER_TABLE:


Parameter_Name:     res_activity        res_working_zone    
Description:        "resistor activity"   "resistor working zone"  
Data_Type:          real            real
Default_Value:      1.0             1.0
Limits:             -               -
Vector:              no              no
Vector_Bounds:       -               -
Null_Allowed:       yes             yes

PARAMETER_TABLE:


Parameter_Name:     res_i        res_u    
Description:        "resistor current"   "resistor voltage"  
Data_Type:          real            real
Default_Value:      1.0             1.0
Limits:             -               -
Vector:              no              no
Vector_Bounds:       -               -
Null_Allowed:       yes             yes

PARAMETER_TABLE:


Parameter_Name:     res_power_over_unit       		 
Description:        "power_over_unit"   
Data_Type:          real            		
Default_Value:      0.0            		
Limits:             -               
Vector:              no              
Vector_Bounds:       -             
Null_Allowed:       yes           


